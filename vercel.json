{
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/style.css", "destination": "/public/style.css" },
    { "source": "/script.js", "destination": "/public/script.js" },
    { "source": "/favicon.ico", "destination": "/public/favicon.ico" },
    { "source": "/dong-bear.png", "destination": "/public/dong-bear.png" },
    { "source": "/voters.json", "destination": "/public/voters.json" },
    { "source": "/index.html", "destination": "/public/index.html" },
    { "source": "/bets.html", "destination": "/public/bets.html" },
    { "source": "/results.html", "destination": "/public/results.html" },
    { "source": "/voters.html", "destination": "/public/voters.html" },
    { "source": "/", "destination": "/public/index.html" }
  ]
}
